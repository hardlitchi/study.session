package io.github.hardlitchi.rest.user.bean;

import java.sql.Timestamp;

import lombok.Data;

/**
 * 【Entityクラス】
 * <br>
 * ユーザ情報.
 * @author hardlitchi
 * @version 1.0
 */
@Data
public class User {

	/** ID(内部識別子). */
	private Long id;
	/** ユーザID(外部識別子). */
	private String username;
	/** ファーストネーム. */
	private String firstname;
	/** ミドルネーム. */
	private String middlename;
	/** ファミリーネーム. */
	private String famillyname;
	/** 性別. */
	private Complex sex;
	/** 作成日時. */
	private Timestamp createTimestamp;
	/** 更新日時. */
	private Timestamp updateTimestamp;
	/** 削除日時. */
	private Timestamp deleteTimestamp;
}
