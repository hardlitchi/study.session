package io.github.hardlitchi.rest.common.entity;

import java.io.Serializable;
import java.sql.Timestamp;

import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.Id;

import lombok.Data;

/**
 * 【Entityクラス】
 * <br>
 * 記念日.
 * @author hardlitchi
 * @version 1.0
 */
@Data
@Entity
public class Anniversary implements Serializable {

	/** ID(内部識別子). */
	@Id
	@GeneratedValue
	private Long id;
	/** 外部キー紐づけ先. */
	private String type;
	/** 外部キー. */
	private Long extractid;
	/** タイトル. */
	private String title;
	/** 詳細. */
	private String description;
	/** 所有者属性. */
	private String ownerattribute;
	/** 所有者. */
	private Long ownerid;
	/** 作成日時. */
	private Timestamp createTimestamp;
	/** 更新日時. */
	private Timestamp updateTimestamp;
	/** 削除日時. */
	private Timestamp deleteTimestamp;
	/** タイトル公開設定ID. */
	private Long titleflag;
	/** 作成日時公開設定ID. */
	private Long createTimestampflag;
	/** 更新日時公開設定ID. */
	private Long updateTimestampflag;
	/** 削除日時公開設定ID. */
	private Long deleteTimestampflag;
}
