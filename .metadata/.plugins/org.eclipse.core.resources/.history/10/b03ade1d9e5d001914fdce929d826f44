package io.github.hardlitchi.rest.common.entity;

/**
 * 通知対象が継承するInterface.
 * @author hardlitchi
 * @version 1.0
 */
public interface NotifiedEntity {

	/** 情報区分. */
	public String getNotifiedEntityType();
	/** 宛先タイプ. */
	public String getDestinationtype();
	/** 宛先. */
	public String getDestination();
}
