package io.github.hardlitchi.rest.common.entity;

/**
 * 通知対象が継承するInterface.
 * @author hardlitchi
 * @version 1.0
 */
public interface NotifiedEntity {

	/** 情報区分. */
	public String getNotifiedEntityType();
	/** 送り主タイプ. */
	public String getSendertype();
	/** 送り主. */
	public String getSender();
}
