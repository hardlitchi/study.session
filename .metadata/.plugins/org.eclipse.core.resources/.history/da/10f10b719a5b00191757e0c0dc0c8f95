package io.github.hardlitchi.rest.common.entity;

import java.io.Serializable;
import java.sql.Timestamp;

import javax.persistence.MappedSuperclass;

import lombok.AllArgsConstructor;
import lombok.Builder;
import lombok.Data;
import lombok.NoArgsConstructor;
import lombok.ToString;

@Data
@AllArgsConstructor
@NoArgsConstructor
@Builder
@ToString
@MappedSuperclass
public class AbstractEntity implements Serializable {

	/** シリアルバージョン. */
	private static final long serialVersionUID = -1604614215038649739L;
	/** 作成日時. */
	private Timestamp createTimestamp;
	/** 更新日時. */
	private Timestamp updateTimestamp;
	/** 削除日時. */
	private Timestamp deleteTimestamp;
	/** タイトル公開設定ID. */
	private Long titleflag;
	/** 作成日時公開設定ID. */
	private Long createTimestampflag;
	/** 更新日時公開設定ID. */
	private Long updateTimestampflag;
	/** 削除日時公開設定ID. */
	private Long deleteTimestampflag;

}
