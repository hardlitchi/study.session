package io.github.hardlitchi.rest.user.bean;

import lombok.Data;

/**
 * 複素数クラス.
 * @author hardlitchi
 * @version 1.0
 */
@Data
public class Complex {

	/** 実数部. */
	private double real;
	/** 虚数部. */
	private double image;

	public Complex add(Complex complex) {
		this.real += complex.getReal();
		this.image += complex.getImage();

		return this;
	}
}
