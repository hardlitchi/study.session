package io.github.hardlitchi.rest.user.bean;

import lombok.Data;

@Data
public class User {

	/** ID(内部識別子). */
	private Long id;
	/** ユーザID(外部識別子). */
	private String userid;
	/** ニックネーム. */
	private String displayname;
	/** ファーストネーム. */
	private String firstname;
	/** ミドルネーム. */
	private String middlename;
	/** ファミリーネーム. */
	private String famillyname;
}
